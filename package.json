{
  "name": "socialhub",
  "version": "0.0.0",
  "description": "socialhub",
  "main": "index.js",
  "scripts": {
    "build-js": "browserify main.js -t [ reactify --everything ] | uglifyjs -mc > build/bundle.js",
    "build-css": "cat css/*.css > build/bundle.css",
    "build": "npm run build-js && npm run build-css",
    "watch-js": "watchify main.js -t [ reactify --everything ] -o build/bundle.js -dv",
    "watch-css": "catw css/*.css -o build/bundle.css -v",
    "watch": "npm run watch-js & npm run watch-css",
    "start": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fraserxu/socialhub.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fraserxu/socialhub/issues"
  },
  "homepage": "https://github.com/fraserxu/socialhub",
  "devDependencies": {
    "catw": "^0.2.0",
    "reactify": "^0.14.0",
    "watchify": "^1.0.6"
  },
  "dependencies": {
    "axios": "^0.3.1",
    "express": "^4.9.5",
    "jsonp": "0.0.4",
    "react": "^0.11.2"
  }
}
